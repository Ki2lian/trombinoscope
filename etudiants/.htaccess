<FilesMatch "\.(txt|psd|log|sh|csv)$">
Order allow,deny
Deny from all

</FilesMatch>

RewriteEngine on

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^(([A-Za-z0-9\-_]+/)*[A-Za-z0-9\-_]+)?$ $1.php

<FilesMatch "administration.php">
	AuthName "Fichier autoris√© uniquement pour les administrateurs du site"
	AuthType Basic
	AuthUserFile "/home/etudiants/www/.htpasswd"
	Require valid-user
</FilesMatch>